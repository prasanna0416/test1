pipeline {
  agent any 
  tools {
    maven 'm3-9-3' /* Same maven name as configured in Jenkins */
  }
  stages {
    stage ('Initialize') {
      steps {
        sh 'echo "PATH = ${env.PATH}"' // Use env.PATH to access the PATH environment variable
        sh 'echo "M2_HOME = ${env.M2_HOME}"' // Use env.M2_HOME to access the M2_HOME environment variable
      }
    }
    stage ('Check-Git-Secrets') {
      steps {
        sh '/usr/bin/sshpass -p poiuyt ssh prasanna@192.168.206.133 "sudo docker run raphaelareya/gitleaks:latest -r https://github.com/prasanna0416/test1.git"'
      }
    }
  }
}
